{
  "manifest_version": 2,
  "name": "GoLink",
  "version": "0.0.0.7",
  "description": "Go to your link!",
  "omnibox": { "keyword" : "go/" },
  "background": {
        "scripts": ["background.js"]
      },
  "content_scripts": [
    {
	"matches": [
      	    "<all_urls>"
        ],
        "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "go.png"
  }
}

